@model SinglePostViewModel
@{
    ViewData["Title"] = "Details";
}

<section id="blogPost">
    <div id="single-blogPost">
        <div id="single-blogPost-leftside">
            <a href="">
                <img src="~/uploads/images/@Model.Post.ImagePath" alt="">
            </a>
            <p class="post-meta">
                <span>by <a class="post-author" href="">John Doe</a></span>
                <span>on <a class="post-date" style="border-right: 1px solid #999999; padding-right: 5px;" href="">@Model.Post.CreatedDate.ToString("MM d, yyyy")</a></span>
                <span>2  <a class="post-comment" href="">Comments</a></span>
            </p>
            <h4 class="post-title"><a href="">@Model.Post.Title</a></h4>
            <div class="post-body">
                @Html.Raw(Model.Post.Body)

                <div class="post-author-detail">
                    <div class="post-author-media">
                        <img style="height: 60px; width: 60px;" src="data:image/jpeg;base64," alt="">
                    </div>

                    <div class="post-author-body">
                        <div class="author-header">
                            <div class="author-header-leftside">
                                <span class="author-detail">AUTHOR</span>
                                <h4 class="author name">John Doe</h4>
                            </div>
                            <div class="author-header-rightside">
                                <a href="#">View all posts by John Doe <i class="fa-solid fa-arrow-right"></i></a>
                            </div>
                        </div>

                        <div class="author-content">
                            <p style="font-size: 14px;
                              color: #666666;
                              width: 750px;
                              line-height: 25px;
                              margin-left: 20px;
                              margin-top: 0px;">
                                Praesent dapibus, neque id cursus faucibus, tortor neque egestas auguae, eu vulputate magna eros euerat. Aliquam erat volutpat.
                            </p>
                        </div>
                    </div>
                </div>
            </div>

            <div class="releated-posts">
                <h3>Related Posts</h3>
                <div class="container">


                    @foreach (var item in Model.RelatedPosts)
                    {
                        <div data-aos="flip-left" class="posts">
                            <a href=""><img src="~/uploads/images/@item.ImagePath" /></a>
                            <h4><a class="post-title" href="">@item.Title</a></h4>
                            <a class="read-more" href="">Read More <i class="fa-solid fa-arrow-right"></i></a>
                        </div>
                    }






                </div>
            </div>

            <div class="comment-section">
                <h3>3 Comments</h3>
                <div class="comment">
                    <div class="author-media">
                        <img src="data:image/jpeg;base64," alt="">
                    </div>
                    <div class="comment-content">
                        <span class="comment-date">November 9, 2018 at 2:19 pm</span>
                        <h4 class="author-name">Jimmy Pearson</h4>
                        <p>Sed pretium, ligula sollicitudin laoreet viverra, tortor libero sodales leo, eget blandit nunc tortor eu nibh. Nullam mollis. Ut justo. Suspendisse potenti.</p>
                        <a href="">Reply</a>
                    </div>
                </div>
                <ul>
                    <div class="comment">
                        <div class="author-media">
                            <img src="data:image/jpeg;base64," alt="">
                        </div>
                        <div class="comment-content">
                            <span class="comment-date">November 9, 2018 at 2:19 pm</span>
                            <h4 class="author-name">Jimmy Pearson</h4>
                            <p>Sed pretium, ligula sollicitudin laoreet viverra, tortor libero sodales leo, eget blandit nunc tortor eu nibh. Nullam mollis. Ut justo. Suspendisse potenti.</p>
                            <a href="">Reply</a>
                        </div>
                    </div>
                </ul>
            </div>

            <div class="write-comment">
                <h3>Leave A Reply</h3>
                <p>Your email address will not be published. Required fields are marked *</p>
                <form action="">
                    <textarea name="" id="" cols="100" rows="10" placeholder="Comment *"></textarea><br>
                    <input type="text" placeholder="Name *">
                    <input style="margin-left: 20px;" type="email" placeholder="Email *"><br><br>
                    <button type="submit">POST COMMENT <i class="fa-solid fa-arrow-right"></i></button>
                </form>
            </div>
        </div>

        <div id="single-blogPost-rightside">
            <div class="search">
                <input type="text" placeholder="Search in Blog">
                <button><i class="fa-solid fa-magnifying-glass"></i></button>
            </div>

            <div class="dropdown">
                <h3><button>Blog Categories</button></h3>
                <div class="dropdown-content">
                    <ul>
                        <li><a class="category-blog" href="">Computers</a></li>
                        <li><a class="category-blog" href="">Tablets</a></li>
                        <li><a class="category-blog" href="">Tv</a></li>
                    </ul>
                </div>
            </div>

            <div class="dropdown">
                <h3><button>Popular Posts</button></h3>
                <div class="dropdown-content">
                    <div class="popular-post">
                        <div class="post-media">
                            <img src="data:image/jpeg;base64," alt="">
                        </div>
                        <div class="post-content">
                            <a class="post-date" href="">Nov 22, 2020</a>
                            <h4 class="post-title"><a href="">The Best Choice For Spending Time</a></h4>
                        </div>
                    </div>

                </div>
            </div>

            <div class="dropdown">
                <h3><button>About Us</button></h3>
                <div class="dropdown-content">
                    <p>
                        Lorem ipsum dolor sit amet consectetur adipisicing elit. Quidem nam iure aspernatur dolorum cum facilis porro dolorem consequatur numquam asperiores!
                    </p>
                </div>
            </div>

            <div class="dropdown">
                <h3><button>Tag Cloud</button></h3>
                <div class="dropdown-content">
                    @foreach (var item in Model.Post.TagCloud)
                    {
                        <a href="">@item.PostTag.Name</a>
                    }

                </div>
            </div>
        </div>


    <</div>
</section>

